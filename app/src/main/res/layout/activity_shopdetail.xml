<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg">

    <LinearLayout
        android:id="@+id/view_bottom"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_share"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal"
            android:layout_weight="4"
            android:background="@drawable/tv_bg_reset"
            android:drawablePadding="3dp"
            android:drawableTop="@mipmap/icon_share"
            android:gravity="center"
            android:paddingTop="5dp"
            android:text="分享"
            android:textColor="#333333"
            android:textSize="10sp" />

        <View
            android:layout_width="0.5dp"
            android:layout_height="match_parent"
            android:background="@color/color_line" />

        <TextView
            android:id="@+id/tv_collection"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal"
            android:layout_weight="4"
            android:background="@drawable/tv_bg_reset"
            android:drawablePadding="3dp"
            android:drawableTop="@drawable/icon_collection"
            android:gravity="center"
            android:paddingTop="5dp"
            android:text="收藏"
            android:textColor="#333333"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/tv_yuyue"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal"
            android:layout_weight="7"
            android:background="@drawable/tv_bg_ok"
            android:gravity="center"
            android:paddingTop="5dp"
            android:text="预约看铺"
            android:textColor="#ffffff"
            android:textSize="15sp" />

        <View
            android:layout_width="0.5dp"
            android:layout_height="match_parent"
            android:background="#ffffff" />

        <TextView
            android:id="@+id/tv_call"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal"
            android:layout_weight="7"
            android:background="@drawable/tv_bg_ok"
            android:gravity="center"
            android:paddingTop="5dp"
            android:text="咨询房东"
            android:textColor="#ffffff"
            android:textSize="15sp" />
    </LinearLayout>


    <View
        android:id="@+id/view_bottom_line"
        style="@style/view_line"
        android:layout_above="@id/view_bottom" />

    <com.finance.winport.view.PositionScrollView
        android:id="@+id/sv_all"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/view_bottom_line"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <com.youth.banner.Banner
                android:id="@+id/img_banner"
                android:layout_width="match_parent"
                android:layout_height="202dp"
                app:indicator_drawable_selected="@drawable/dot_banner_select"
                app:indicator_drawable_unselected="@drawable/dot_banner_unselect"
                app:indicator_height="6dp"
                app:indicator_margin="5dp"
                app:indicator_width="6dp"
                android:background="@drawable/default_image_logo"></com.youth.banner.Banner>


            <TextView
                android:id="@+id/tv_name"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="#ffffff"
                android:drawableLeft="@mipmap/icon_firm"
                android:drawablePadding="5dp"
                android:gravity="center_vertical"
                android:paddingLeft="15dp"
                android:text="由 小二 －－－ 于2016.02.03 实勘核实"
                android:textColor="#999999"
                android:textSize="12sp" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="#ffffff"
                android:paddingBottom="15dp"
                android:paddingLeft="15dp"
                android:paddingTop="15dp">

                <RelativeLayout
                    android:id="@+id/rl_address"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tv_rent_type"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:text="房东直发"
                        android:textColor="#FF8C9AC9"
                        android:textSize="10sp"
                        android:background="@drawable/bg_type"
                        android:gravity="center"
                        android:lineSpacingExtra="0dp"
                        android:includeFontPadding="false"/>
                    <TextView
                        android:id="@+id/tv_shop_address"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="15dp"
                        android:text="－－"
                        android:textColor="#333333"
                        android:textSize="20sp" />
                </RelativeLayout>


                <TextView
                    android:id="@+id/tv_scan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rl_address"
                    android:layout_marginTop="10dp"
                    android:text="1980浏览"
                    android:textColor="#999999"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_lianxi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@id/tv_scan"
                    android:layout_marginLeft="20dp"
                    android:layout_toRightOf="@id/tv_scan"
                    android:text="23090联系"
                    android:textColor="#999999"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_jiucuo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_alignTop="@id/tv_scan"
                    android:layout_marginRight="15dp"
                    android:text="纠错"
                    android:textColor="#999999"
                    android:textSize="12sp" />

                <com.finance.winport.view.tagview.TagCloudLayout
                    android:id="@+id/tag"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tv_scan"
                    android:layout_marginTop="10dp"></com.finance.winport.view.tagview.TagCloudLayout>

                <View
                    android:id="@+id/view_line"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@id/tag"
                    android:layout_marginTop="15dp"
                    android:background="@color/color_line" />

                <TextView
                    android:id="@+id/tv_name_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/view_line"
                    android:layout_marginTop="15dp"
                    android:text="价格"
                    android:textColor="#999999"
                    android:textSize="14sp" />

                <ImageView
                    android:id="@+id/imv_change"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/view_line"
                    android:layout_marginLeft="77dp"
                    android:layout_marginTop="13dp"
                    android:layout_toRightOf="@id/tv_name_price"
                    android:scaleType="centerInside"
                    android:src="@mipmap/icon_switch" />

                <TextView
                    android:id="@+id/tv_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tv_name_price"
                    android:layout_marginTop="10dp"
                    android:text="----"
                    android:textColor="#FF151515"
                    android:textSize="12sp"/>

                <View
                    android:id="@+id/view_line_one"
                    android:layout_width="0.5dp"
                    android:layout_height="42dp"
                    android:layout_below="@id/view_line"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_toRightOf="@id/imv_change"
                    android:background="@color/color_line" />

                <TextView
                    android:id="@+id/tv_name_zhuanprice"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/view_line"
                    android:layout_marginLeft="13dp"
                    android:layout_marginTop="15dp"
                    android:layout_toRightOf="@id/view_line_one"
                    android:text="转让费"
                    android:textColor="#999999"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/tv_zhuanprice"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@id/tv_name_zhuanprice"
                    android:layout_below="@id/tv_name_zhuanprice"
                    android:layout_marginTop="10dp"
                    android:text="---"
                    android:textColor="#FF151515"
                    android:textSize="12sp" />

                <View
                    android:id="@+id/view_line_two"
                    android:layout_width="0.5dp"
                    android:layout_height="42dp"
                    android:layout_below="@id/view_line"
                    android:layout_marginLeft="56dp"
                    android:layout_marginTop="15dp"
                    android:layout_toRightOf="@id/tv_name_zhuanprice"
                    android:background="@color/color_line" />

                <TextView
                    android:id="@+id/tv_name_area"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/view_line"
                    android:layout_marginLeft="13dp"
                    android:layout_marginTop="15dp"
                    android:layout_toRightOf="@id/view_line_two"
                    android:text="面积"
                    android:textColor="#999999"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/tv_area"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@id/tv_name_area"
                    android:layout_below="@id/tv_name_area"
                    android:layout_marginTop="10dp"
                    android:text="---"
                    android:textColor="#FF5851"
                    android:textSize="18sp" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="10dp" />
            <!-- 商铺周边 -->
            <LinearLayout
                android:id="@+id/ll_near"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    style="@style/second_title_arrow"
                    android:text="商铺周边" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="160dp"
                    android:background="#ff0000" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#ffffff"
                    android:orientation="horizontal"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="240"
                            android:textColor="#333333"
                            android:textSize="15sp"
                            android:visibility="gone"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="餐饮"
                            android:textColor="#999999"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="240"
                            android:textColor="#333333"
                            android:textSize="15sp"
                            android:visibility="gone"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="购物"
                            android:textColor="#999999"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="240"
                            android:textColor="#333333"
                            android:textSize="15sp"
                            android:visibility="gone"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="酒店"
                            android:textColor="#999999"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="240"
                            android:textColor="#333333"
                            android:textSize="15sp"
                            android:visibility="gone"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="小区"
                            android:textColor="#999999"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="240"
                            android:textColor="#333333"
                            android:textSize="15sp"
                            android:visibility="gone"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="学校"
                            android:textColor="#999999"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="240"
                            android:textColor="#333333"
                            android:textSize="15sp"
                            android:visibility="gone"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="交通"
                            android:textColor="#999999"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <View
                android:id="@+id/view_space_linpu"
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:visibility="gone" />
            <!-- 邻铺信息-->
            <LinearLayout
                android:id="@+id/ll_linpu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <TextView
                    style="@style/second_title_arrow"
                    android:drawableRight="@null"
                    android:text="邻铺信息" />

                <View
                    style="@style/view_line"
                    android:layout_marginLeft="15dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#ffffff"
                    android:orientation="horizontal"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp">

                    <com.finance.winport.view.home.NearShop
                        android:id="@+id/view_shop_one"
                        android:layout_width="72dp"
                        android:layout_height="wrap_content"
                        android:visibility="gone"></com.finance.winport.view.home.NearShop>

                    <com.finance.winport.view.home.NearShop
                        android:id="@+id/view_shop_two"
                        android:layout_width="72dp"
                        android:layout_height="wrap_content"
                        android:visibility="gone"></com.finance.winport.view.home.NearShop>

                    <com.finance.winport.view.home.NearShop
                        android:id="@+id/view_shop_three"
                        android:layout_width="72dp"
                        android:layout_height="wrap_content"
                        android:visibility="gone"></com.finance.winport.view.home.NearShop>

                    <com.finance.winport.view.home.NearShop
                        android:id="@+id/view_shop_four"
                        android:layout_width="72dp"
                        android:layout_height="wrap_content"
                        android:visibility="gone"></com.finance.winport.view.home.NearShop>

                    <com.finance.winport.view.home.NearShop
                        android:id="@+id/view_shop_five"
                        android:layout_width="72dp"
                        android:layout_height="wrap_content"
                        android:visibility="gone"></com.finance.winport.view.home.NearShop>
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="10dp" />
            <!-- 门面数据-->
            <LinearLayout
                android:id="@+id/ll_menmianshuju"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    style="@style/second_title_arrow"
                    android:drawableRight="@null"
                    android:text="门面数据" />

                <View
                    style="@style/view_line"
                    android:layout_marginLeft="15dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#ffffff"
                    android:orientation="horizontal"
                    android:paddingBottom="19dp"
                    android:paddingTop="15dp">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="30dp"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_cenggao"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="8.9m"
                            android:textColor="#333333"
                            android:textSize="15sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawableBottom="@drawable/bg_cenggao"
                            android:drawablePadding="2dp"
                            android:text="层高"
                            android:textColor="#999999"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="30dp"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_miankuan"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="--"
                            android:textColor="#333333"
                            android:textSize="15sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawableBottom="@drawable/bg_miankuan"
                            android:drawablePadding="2dp"
                            android:text="面宽"
                            android:textColor="#999999"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="30dp"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_jinshen"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="10m"
                            android:textColor="#333333"
                            android:textSize="15sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawableBottom="@drawable/bg_jinshen"
                            android:drawablePadding="2dp"
                            android:text="进深"
                            android:textColor="#999999"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="29dp"
                        android:scaleType="centerInside"
                        android:src="@mipmap/icon_facade" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:id="@+id/view_space_jingyingfanwei"
                android:layout_width="match_parent"
                android:layout_height="10dp" />


            <LinearLayout
                android:id="@+id/ll_jingyingfanwei"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:orientation="vertical">

                <TextView
                    style="@style/second_title_arrow"
                    android:drawableRight="@null"
                    android:text="经营范围" />

                <View
                    style="@style/view_line"
                    android:layout_marginLeft="15dp" />

                <com.finance.winport.view.tagview.TagCloudLayout
                    android:id="@+id/tg_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="15dp"
                    app:lineSpacing="10dp"
                    app:tagSpacing="10dp" />

            </LinearLayout>

            <View
                android:id="@+id/view_space_jingyingfeiyong"
                android:layout_width="match_parent"
                android:layout_height="10dp" />

            <LinearLayout
                android:id="@+id/ll_jingyingfeiyong"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    style="@style/second_title_arrow"
                    android:drawableRight="@null"
                    android:text="营运费用" />

                <View
                    style="@style/view_line"
                    android:layout_marginLeft="15dp" />

                <LinearLayout
                    android:id="@+id/ll_jingyingfeiyongone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#ffffff"
                    android:orientation="vertical"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="77dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_horizontal"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tv_dianfei"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="3.20"
                                android:textColor="#333333"
                                android:textSize="30sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="电费(元/度)"
                                android:textColor="#999999"
                                android:textSize="12sp" />
                        </LinearLayout>

                        <View
                            android:layout_width="0.5dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="15dp"
                            android:background="@color/color_line" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_horizontal"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tv_shuifei"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="6.0"
                                android:textColor="#333333"
                                android:textSize="30sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="水费(元/吨)"
                                android:textColor="#999999"
                                android:textSize="12sp" />
                        </LinearLayout>

                    </LinearLayout>

                    <View
                        style="@style/view_line"
                        android:layout_marginLeft="15dp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="77dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_horizontal"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tv_ranqi"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="6.0"
                                android:textColor="#333333"
